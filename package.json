{
  "name": "express-signed-url",
  "version": "2.0.0",
  "packageManager": "yarn@3.1.1",
  "description": "Tiny express library for signing urls and validating them based on secret key",
  "keywords": [
    "express",
    "url",
    "signature",
    "sign",
    "signed url"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "mocha dist/tests"
  },
  "bugs": {
    "url": "https://github.com/schmollmolch/express-signed-url/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/MIT"
    }
  ],
  "author": {
    "name": "Christian Scheja",
    "email": "cscheja@gmx.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/schmollmolch/express-signed-url.git"
  },
  "main": "./dist/index",
  "types": "./types.d.ts",
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/mocha": "9.0.0",
    "@types/node": "16.11.11",
    "@types/request": "^2.48.7",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "4.17.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "mocha": "9.1.3",
    "prettier": "^2.5.0",
    "request": "2.88.2",
    "typescript": "4.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix",
      "git add"
    ]
  }
}
